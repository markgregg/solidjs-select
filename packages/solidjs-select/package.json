{
  "name": "solidjs-select",
  "version": "0.8.3",
  "description": "Select control for solid-js",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.js",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "rollup -c",
    "watch": "rollup -cw"
  },
  "keywords": [
    "solid-jds",
    "select",
    "UI",
    "paste",
    "lookup",
    "typeahead",
    "virtualised",
    "virtual"
  ],
  "author": "Mark Gregg <gregg.mark@gmail.com> (https://github.com/markgregg)",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.6.9"
  },
  "dependencies": {
    "solid-icons": "^1.0.4",
    "solidjs-virtualisation": "^0.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "autoprefixer": "^10.4.13",
    "prettier": "2.8.3",
    "rollup": "^3.12.0",
    "rollup-preset-solid": "^2.0.1",
    "solid-js": "^1.6.9",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0"
  }
}
